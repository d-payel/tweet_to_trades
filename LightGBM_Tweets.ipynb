      }
    },
    {
      "cell_type": "code",
      "source": [
        "lgbm_pipeline = Pipeline([\n",
        "    ('tfidf', TfidfVectorizer(ngram_range=(1, 2), min_df=3, stop_words='english')),\n",
        "    ('smote', SMOTE(random_state=42)),\n",
        "    ('classifier', lgb.LGBMClassifier(random_state=42, n_jobs=-1))\n",
        "])\n"
      ],
      "metadata": {
        "id": "jwK8HlfSnTEa"
      },
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "lgbm_param_grid = {\n",
        "    'classifier__n_estimators': [200, 300, 400],\n",
        "    'classifier__learning_rate': [0.05, 0.1],\n",
        "    'classifier__num_leaves': [15, 20, 31],  # Focus on smaller, less complex trees\n",
        "    'classifier__max_depth': [5, 7, 10],      # Enforce a maximum depth to prevent runaways\n",
        "    'classifier__reg_alpha': [0.1, 0.5, 1],    # L1 (Lasso) Crucial for overfitting\n",
        "    'classifier__reg_lambda': [0.1, 0.5, 1],   # L2 Ridge Crucial for overfitting\n",
        "    'classifier__min_gain_to_split': [0.0, 0.1, 0.5], 
        "    'classifier__subsample': [0.8, 0.9],\n",
        "    'classifier__colsample_bytree': [0.8, 0.9]\n",
        "}"
      ],
      "metadata": {
        "id": "plzVDTcFnX-z"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "# 3. Set up the Randomized Search\n",
        "cv = StratifiedKFold(n_splits=5, shuffle=True, random_state=42)\n",
        "lgbm_random_search = RandomizedSearchCV(\n",
        "    estimator=lgbm_pipeline,\n",
        "    param_distributions=lgbm_param_grid,\n",
        "    n_iter=50,  # Try a good number of combinations\n",
        "    scoring='f1_weighted',\n",
        "    cv=cv,\n",
        "    verbose=2,\n",
        "    random_state=42,\n",
        "    n_jobs=-1\n",
        ")\n"
